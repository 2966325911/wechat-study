/* pages/contact/contact.wxss */
.item_header {
  margin-top: 20rpx;
}
.item {
  background-color: white;
  padding: 20rpx 20rpx;
  border-bottom: 1rpx solid #ccc
}

.item_header :last-child {
  border-bottom: 0rpx
} 

.kf_button{
  background-color: rgba(255, 255, 255, 0);
  border: 0px;
  height: 100rpx;
  right: 0;
  bottom: 20rpx;
  position: fixed;
}
.kf_button::after{
  border: 0px;
}
.kf_image{
  z-index: 9999;
  width: 100rpx;
  height: 100rpx;
}

.shareBtn{
  margin-top: 10rpx
}